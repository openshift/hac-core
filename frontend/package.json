{
  "name": "hac-core",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "analyze": "NODE_ENV=production webpack --config config/prod.webpack.config.js --env analyze=true",
    "build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "yarn build && yarn lint && yarn test",
    "lint": "yarn lint:ts && yarn lint:sass",
    "lint:ts": "eslint . --ext .js,.jsx,.ts,.tsx --color",
    "lint:ts:fix": "yarn lint:ts --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "yarn deploy",
    "dev": "yarn start:proxy:beta",
    "start": "webpack serve --config config/dev.webpack.config.js",
    "start:k8s": "ENVIRONMENT=prod BETA=true PROXY=true yarn start",
    "start:proxy": "PROXY=true BETA=true webpack serve --config config/dev.webpack.config.js",
    "start:proxy:beta": "PROXY=true BETA=true webpack serve --config config/dev.webpack.config.js",
    "start:beta": "BETA=true npm start",
    "start:standalone": "STANDALONE=true npm start",
    "test": "jest -w 1",
    "verify": "yarn build && yarn lint && yarn test"
  },
  "dependencies": {
    "@openshift/dynamic-plugin-sdk": "^2.0.1",
    "@openshift/dynamic-plugin-sdk-extensions": "^1.1.0",
    "@openshift/dynamic-plugin-sdk-utils": "^1.1.1",
    "@patternfly/patternfly": "^4.185.1",
    "@patternfly/quickstarts": "^2.3.1",
    "@patternfly/react-catalog-view-extension": "^4.53.16",
    "@patternfly/react-core": "^4.202.16",
    "@patternfly/react-table": "^4.71.16",
    "@redhat-cloud-services/frontend-components": "^3.8.10",
    "@redhat-cloud-services/frontend-components-notifications": "^3.2.5",
    "@redhat-cloud-services/frontend-components-utilities": "^3.2.14",
    "@unleash/proxy-client-react": "^3.4.1",
    "ajv": "^6.12.6",
    "chalk": "^2.4.2",
    "classnames": "^2.3.1",
    "comment-json": "^4.1.1",
    "copy-webpack-plugin": "^10.2.0",
    "find-up": "^4.1.0",
    "lodash-es": "^4.17.21",
    "pluralize": "^8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.14.2",
    "react-redux": "7.2.6",
    "react-router": "^6.3.0",
    "react-router-dom": "6.3.0",
    "read-pkg": "^5.2.0",
    "redux": "4.1.0",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.2",
    "redux-thunk": "^2.4.1",
    "semver": "^6.3.0",
    "unleash-proxy-client": "^2.3.0",
    "webpack-virtual-modules": "^0.4.3"
  },
  "devDependencies": {
    "@patternfly/react-virtualized-extension": "^4.53.2",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.2.3",
    "@redhat-cloud-services/frontend-components-config": "^4.6.14",
    "@scalprum/core": "0.4.0-beta.6",
    "@scalprum/react-core": "0.4.0-beta.6",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.5.1",
    "@types/pluralize": "^0.0.29",
    "@types/react-redux": "7.x",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-virtualized": "^9.21.21",
    "@types/redux-logger": "3.0.6",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.0.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.29.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.5",
    "jest-fetch-mock": "^3.0.3",
    "prop-types": "15.7.2",
    "react-virtualized": "^9.22.3",
    "stylelint": "13.13.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.19.0",
    "ts-jest": "^28.0.3",
    "ts-node": "10.8.0",
    "typesafe-actions": "^5.1.0",
    "typescript": "4.4.4",
    "webpack-bundle-analyzer": "4.4.2"
  },
  "insights": {
    "appname": "hac-core",
    "buildrepo": "git@github.com:RedHatInsights/hac-core-frontend-build.git"
  }
}
