{
  "name": "hac-core",
  "version": "1.1.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "analyze": "NODE_ENV=production webpack --config config/prod.webpack.config.js --env analyze=true",
    "build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "yarn build && yarn lint && yarn test",
    "lint": "yarn lint:ts && yarn lint:sass",
    "lint:ts": "eslint . --ext .js,.jsx,.ts,.tsx --color",
    "lint:ts:fix": "yarn lint:ts --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "yarn deploy",
    "dev": "yarn start:proxy:beta",
    "start": "webpack serve --config config/dev.webpack.config.js",
    "start:k8s": "ENVIRONMENT=prod BETA=true PROXY=true yarn start",
    "start:proxy": "PROXY=true webpack serve --config config/dev.webpack.config.js",
    "start:proxy:beta": "PROXY=true BETA=true webpack serve --config config/dev.webpack.config.js",
    "start:beta": "BETA=true npm start",
    "start:standalone": "STANDALONE=true npm start",
    "test": "jest -w 1",
    "verify": "yarn build && yarn lint && yarn test"
  },
  "dependencies": {
    "@openshift/dynamic-plugin-sdk": "^4.0.0",
    "@openshift/dynamic-plugin-sdk-extensions": "^1.3.0",
    "@openshift/dynamic-plugin-sdk-utils": "^4.0.0",
    "@patternfly/patternfly": "^5.1.0",
    "@patternfly/quickstarts": "^5.0.0",
    "@patternfly/react-catalog-view-extension": "^5.0.0",
    "@patternfly/react-core": "^5.1.1",
    "@patternfly/react-table": "^5.1.1",
    "@redhat-cloud-services/frontend-components": "^4.0.14",
    "@redhat-cloud-services/frontend-components-notifications": "^4.0.4",
    "@redhat-cloud-services/frontend-components-utilities": "^4.0.2",
    "@unleash/proxy-client-react": "^3.4.1",
    "ajv": "^6.12.6",
    "chalk": "^2.4.2",
    "classnames": "^2.3.1",
    "comment-json": "^4.1.1",
    "copy-webpack-plugin": "^10.2.0",
    "find-up": "^4.1.0",
    "lodash-es": "^4.17.21",
    "pluralize": "^8.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.14.2",
    "react-redux": "7.2.6",
    "react-router": "^6.3.0",
    "react-router-dom": "6.3.0",
    "read-pkg": "^5.2.0",
    "redux": "4.1.0",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.2",
    "redux-thunk": "^2.4.1",
    "semver": "^6.3.0",
    "unleash-proxy-client": "^2.3.0",
    "webpack-virtual-modules": "^0.4.3"
  },
  "devDependencies": {
    "@patternfly/react-virtualized-extension": "^5.0.0",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^2.0.3",
    "@redhat-cloud-services/frontend-components-config": "^6.0.5",
    "@scalprum/core": "0.6.0",
    "@scalprum/react-core": "0.6.0",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.5.1",
    "@types/pluralize": "^0.0.29",
    "@types/react-redux": "7.x",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-virtualized": "^9.21.21",
    "@types/redux-logger": "3.0.6",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.51.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.5",
    "jest-fetch-mock": "^3.0.3",
    "prop-types": "15.7.2",
    "react-virtualized": "^9.22.3",
    "stylelint": "15.11.0",
    "stylelint-config-recommended-scss": "13.0.0",
    "stylelint-scss": "5.2.1",
    "ts-jest": "^28.0.3",
    "ts-node": "10.8.0",
    "typesafe-actions": "^5.1.0",
    "typescript": "4.9.5",
    "webpack-bundle-analyzer": "4.4.2"
  },
  "insights": {
    "appname": "hac-core",
    "buildrepo": "git@github.com:RedHatInsights/hac-core-frontend-build.git"
  }
}
